{
  "name": "neon-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start",
    "dev": "npx expo start --clear",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "web:dev": "npx expo start --web --clear",
    "web:hot": "EXPO_USE_FAST_REFRESH=true npx expo start --web --clear",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "remove-comments": "node scripts/remove-comments.js",
    "remove-comments:src": "node scripts/remove-comments.js ./src",
    "remove-comments:dry-run": "node scripts/remove-comments.js --dry-run",
    "remove-comments:src-dry": "node scripts/remove-comments.js ./src --dry-run",
    "remove-comments:no-backup": "node scripts/remove-comments.js --no-backup"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@microsoft/signalr": "^8.0.7",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@tanstack/react-query": "^5.81.2",
    "@types/node": "^24.0.4",
    "axios": "^1.10.0",
    "dotenv": "^16.6.0",
    "expo": "~53.0.13",
    "expo-dev-client": "^5.2.1",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.59.0",
    "react-native": "0.79.4",
    "react-native-keychain": "^10.0.0",
    "react-native-safe-area-context": "^5.5.0",
    "react-native-screens": "^4.11.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^30.0.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.6.1",
    "react-test-renderer": "^19.0.0",
    "typescript": "^5.8.3"
  },
  "private": true
}
